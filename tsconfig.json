{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "jsx": "react",
    "lib": ["es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "isolatedModules": true,
    "types": ["react", "react-native"],
    "strict": false
  },
  "include": ["src"]
}

